package com.statcraft.testConnection;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.cloud.netflix.eureka.EnableEurekaClient;
import org.springframework.context.annotation.ComponentScan;

import com.example.contorller.HomeController;

@SpringBootApplication
@EnableEurekaClient
@ComponentScan(basePackageClasses = BottelController.class)
public class TestConnectionApplication {

	public static void main(String[] args) {
		SpringApplication.run(TestConnectionApplication.class, args);
		System.out.println("Main method");
	}

}
