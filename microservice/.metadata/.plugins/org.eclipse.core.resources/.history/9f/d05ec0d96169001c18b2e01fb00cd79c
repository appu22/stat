package com.statcraft.booksStall;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.cloud.netflix.eureka.EnableEurekaClient;
import org.springframework.context.annotation.ComponentScan;

import com.statcraft.controller.BooksController;

@SpringBootApplication
@EnableEurekaClient
@ComponentScan(basepackage = BooksController.class)
public class BooksStallApplication {

	public static void main(String[] args) {
		SpringApplication.run(BooksStallApplication.class, args);
	}

}
